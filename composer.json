{
    "name": "black-lamp/yii2-newsletter",
    "description": "Module for subscribe on newsletter",
    "keywords": ["yii2", "subscribe", "newsletter", "delivery"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/black-lamp/yii2-newsletter",
