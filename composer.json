        "codeception/verify": "*",
        "codeception/specify": "*"
    },
    "authors": [
        {
            "name": "Vladimir Kuprienko",
            "email": "vldmr.kuprienko@gmail.com",
            "role": "lead"
        }
    ],
    "autoload": {
        "psr-4": {
            "bl\\newsletter\\": "src"
        }
    }
